        android:allowBackup="true"
        android:icon="@drawable/btnfixpay"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppCustomTheme2"
        android:name="android.support.multidex.MultiDexApplication"
        >

        <activity android:name=".Activities.SignInActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SignUpActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.DashboardActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.MerchantListActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.ScanActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.MerchantProfileActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.MenuListActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.CardListActivity"
            android:screenOrientation="portrait">



        </activity>
        <activity android:name=".Activities.UserProfileIDActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activities.FinalShareActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activities.MerchantProfileIDActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activities.UploadFundsActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.TermsAndConditionsActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.ForgetPasswordActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.FundWithdrawActivity"
            android:screenOrientation="portrait"></activity>
        <service
            android:name=".Firebase.FirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <service
            android:name=".Firebase.FirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <activity android:name=".Activities.ChangePasswordActivity"></activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="50cf4b609d355b6f7ca018b3411dd6aab2a2ad4b"
            />
    </application>

</manifest>